{
  "name": "iggyvolz/class-properties",
  "description": "C#-style class properties for PHP classes",
  "version": "0.3.0",
  "type": "library",
  "require-dev": {
    "vimeo/psalm": "^5",
    "phpunit/phpunit": "^10",
    "phan/phan": "^5",
    "squizlabs/php_codesniffer": "^3"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Katie Volz",
      "email": "iggyvolz@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "iggyvolz/initializable": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "iggyvolz\\ClassProperties\\": "src/",
      "iggyvolz\\ClassProperties\\examples\\": "examples/",
      "iggyvolz\\ClassProperties\\tests\\": "tests/"
    }
  }
}
